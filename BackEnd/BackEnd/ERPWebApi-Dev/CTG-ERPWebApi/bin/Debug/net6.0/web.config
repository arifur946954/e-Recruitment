<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
    <httpRuntime executionTimeout="3600" />
  </system.web>
  <system.webServer>
    <security>
      <requestFiltering>
        <!-- Limit Upload to 700MB   -->
        <requestLimits maxAllowedContentLength="4000000000" />
      </requestFiltering>
    </security>
    <modules>
      <remove name="WebDAVModule" />
    </modules>
  </system.webServer>
</configuration>